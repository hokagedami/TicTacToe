<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hokage.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartScreenActivity" />
        <activity android:name=".OnePlayerActivity" />
        <activity android:name=".TwoPlayersActivity" />
        <activity android:name=".OptionsActivity" />
        <activity android:name=".ReminderActivity" />
        <activity android:name=".BoardTypeActivity" />
        <activity android:name=".HelpActivity" />
        <activity
            android:name=".ThreeByThreeActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_three_by_three"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".FiveByFiveActivity"></activity>
    </application>

</manifest>